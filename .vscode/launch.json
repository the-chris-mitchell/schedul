    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serve",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/schedul",
            "console": "integratedTerminal",
            "module": "uvicorn",
            "args": ["main:app","--reload"]
        },
        {
            "name": "FFF23: Scrape Dev",
            "type": "python",
            "request": "launch",
            "program": "scrape.py",
            "cwd": "${workspaceFolder}/src/schedul",
            "console": "integratedTerminal",
            "args": ["dev", "fff23"]
        },
        {
            "name": "FFF23: Scrape Prod",
            "type": "python",
            "request": "launch",
            "program": "scrape.py",
            "cwd": "${workspaceFolder}/src/schedul",
            "console": "integratedTerminal",
            "args": ["prod", "fff23"]
        },
    ]
}
